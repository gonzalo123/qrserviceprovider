{
    "name": "gonzalo123/qrserviceprovider",
    "description": "QR code service provider for silex using endroid/qrcode",
    "license": "MIT",
    "keywords": [
        "silex",
        "qrcode",
        "qr"
    ],

    "authors": [
        {
            "name": "Gonzalo Ayuso",
            "email": "gonzalo123@gmail.com"
        }
    ],

    "require": {
        "silex/silex": ">=v1.1.2, <4.0",
        "endroid/qrcode": ">=1.1.3"
    },

    "autoload": {
        "psr-4": {
            "G\\": "src/"
        }
    },

    "config": {
        "bin-dir": "bin"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}